{"variants":[{"paths":["\/documentation\/glib\/sourcefuncs\/sourcefuncsprotocol-implementations"],"traits":[{"interfaceLanguage":"swift"}]}],"schemaVersion":{"major":0,"minor":3,"patch":0},"sections":[],"identifier":{"url":"doc:\/\/GLib\/documentation\/GLib\/SourceFuncs\/SourceFuncsProtocol-Implementations","interfaceLanguage":"swift"},"topicSections":[{"title":"Instance Properties","identifiers":["doc:\/\/GLib\/documentation\/GLib\/SourceFuncs\/check","doc:\/\/GLib\/documentation\/GLib\/SourceFuncs\/dispatch","doc:\/\/GLib\/documentation\/GLib\/SourceFuncs\/finalize","doc:\/\/GLib\/documentation\/GLib\/SourceFuncs\/prepare"],"generated":true},{"title":"Instance Methods","identifiers":["doc:\/\/GLib\/documentation\/GLib\/SourceFuncs\/sourceRemoveByFuncs(userData:)"],"generated":true}],"kind":"article","metadata":{"modules":[{"name":"GLib"}],"role":"collectionGroup","title":"SourceFuncsProtocol Implementations"},"hierarchy":{"paths":[["doc:\/\/GLib\/documentation\/GLib","doc:\/\/GLib\/documentation\/GLib\/SourceFuncs"]]},"references":{"doc://GLib/documentation/GLib/SourceFuncs":{"role":"symbol","title":"SourceFuncs","fragments":[{"kind":"keyword","text":"class"},{"kind":"text","text":" "},{"kind":"identifier","text":"SourceFuncs"}],"abstract":[{"type":"text","text":"The "},{"type":"codeVoice","code":"GSourceFuncs"},{"type":"text","text":" struct contains a table of"},{"type":"text","text":" "},{"type":"text","text":"functions used to handle event sources in a generic manner."}],"identifier":"doc:\/\/GLib\/documentation\/GLib\/SourceFuncs","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"SourceFuncs"}],"url":"\/documentation\/glib\/sourcefuncs"},"doc://GLib/documentation/GLib/MainContext":{"role":"symbol","title":"MainContext","fragments":[{"kind":"keyword","text":"class"},{"kind":"text","text":" "},{"kind":"identifier","text":"MainContext"}],"abstract":[{"type":"text","text":"The "},{"type":"codeVoice","code":"GMainContext"},{"type":"text","text":" struct is an opaque data"},{"type":"text","text":" "},{"type":"text","text":"type representing a set of sources to be handled in a main loop."}],"identifier":"doc:\/\/GLib\/documentation\/GLib\/MainContext","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"MainContext"}],"url":"\/documentation\/glib\/maincontext"},"doc://GLib/documentation/GLib/SourceFuncs/check":{"role":"symbol","title":"check","fragments":[{"kind":"keyword","text":"var"},{"kind":"text","text":" "},{"kind":"identifier","text":"check"},{"kind":"text","text":": "},{"kind":"typeIdentifier","text":"GSourceFuncsCheckFunc","preciseIdentifier":"c:@T@GSourceFuncsCheckFunc"},{"kind":"text","text":"!"}],"abstract":[{"type":"text","text":"Called after all the file descriptors are polled. The source"},{"type":"text","text":" "},{"type":"text","text":"should return "},{"type":"codeVoice","code":"true"},{"type":"text","text":" if it is ready to be dispatched. Note that some"},{"type":"text","text":" "},{"type":"text","text":"time may have passed since the previous prepare function was called,"},{"type":"text","text":" "},{"type":"text","text":"so the source should be checked again here.  Since 2.36 this may"},{"type":"text","text":" "},{"type":"text","text":"be "},{"type":"codeVoice","code":"nil"},{"type":"text","text":", in which case the effect is as if the function always returns"},{"type":"text","text":" "},{"type":"codeVoice","code":"false"},{"type":"text","text":"."}],"identifier":"doc:\/\/GLib\/documentation\/GLib\/SourceFuncs\/check","kind":"symbol","type":"topic","url":"\/documentation\/glib\/sourcefuncs\/check"},"doc://GLib/documentation/GLib/SourceFuncs/finalize":{"role":"symbol","title":"finalize","fragments":[{"kind":"keyword","text":"var"},{"kind":"text","text":" "},{"kind":"identifier","text":"finalize"},{"kind":"text","text":": "},{"kind":"typeIdentifier","text":"GSourceFuncsFinalizeFunc","preciseIdentifier":"c:@T@GSourceFuncsFinalizeFunc"},{"kind":"text","text":"!"}],"abstract":[{"type":"text","text":"Called when the source is finalized. At this point, the source"},{"type":"text","text":" "},{"type":"text","text":"will have been destroyed, had its callback cleared, and have been removed"},{"type":"text","text":" "},{"type":"text","text":"from its [struct"},{"type":"reference","isActive":true,"identifier":"doc:\/\/GLib\/documentation\/GLib\/MainContext"},{"type":"text","text":"], but it will still have its final"},{"type":"text","text":" "},{"type":"text","text":"reference count, so methods can be called on it from within this"},{"type":"text","text":" "},{"type":"text","text":"function."}],"identifier":"doc:\/\/GLib\/documentation\/GLib\/SourceFuncs\/finalize","kind":"symbol","type":"topic","url":"\/documentation\/glib\/sourcefuncs\/finalize"},"doc://GLib/documentation/GLib/sourceRemove":{"role":"symbol","title":"sourceRemove","fragments":[{"kind":"keyword","text":"let"},{"kind":"text","text":" "},{"kind":"identifier","text":"sourceRemove"},{"kind":"text","text":": "},{"kind":"typeIdentifier","text":"Int32","preciseIdentifier":"s:s5Int32V"}],"abstract":[{"type":"text","text":"Use this macro as the return value of a [callback"},{"type":"reference","isActive":true,"identifier":"doc:\/\/GLib\/documentation\/GLib\/SourceFunc"},{"type":"text","text":"] to remove"},{"type":"text","text":" "},{"type":"text","text":"the [struct"},{"type":"reference","isActive":true,"identifier":"doc:\/\/GLib\/documentation\/GLib\/Source"},{"type":"text","text":"] from the main loop."}],"identifier":"doc:\/\/GLib\/documentation\/GLib\/sourceRemove","kind":"symbol","type":"topic","url":"\/documentation\/glib\/sourceremove"},"doc://GLib/documentation/GLib/SourceFuncs/sourceRemoveByFuncs(userData:)":{"role":"symbol","title":"sourceRemoveByFuncs(userData:)","fragments":[{"kind":"keyword","text":"func"},{"kind":"text","text":" "},{"kind":"identifier","text":"sourceRemoveByFuncs"},{"kind":"text","text":"("},{"kind":"externalParam","text":"userData"},{"kind":"text","text":": "},{"kind":"typeIdentifier","text":"gpointer","preciseIdentifier":"c:@T@gpointer"},{"kind":"text","text":"?) -> "},{"kind":"typeIdentifier","text":"Bool","preciseIdentifier":"s:Sb"}],"abstract":[{"type":"text","text":"Removes a source from the default main loop context given the"},{"type":"text","text":" "},{"type":"text","text":"source functions and user data. If multiple sources exist with the"},{"type":"text","text":" "},{"type":"text","text":"same source functions and user data, only one will be destroyed."}],"identifier":"doc:\/\/GLib\/documentation\/GLib\/SourceFuncs\/sourceRemoveByFuncs(userData:)","kind":"symbol","type":"topic","url":"\/documentation\/glib\/sourcefuncs\/sourceremovebyfuncs(userdata:)"},"doc://GLib/documentation/GLib/sourceContinue":{"role":"symbol","title":"sourceContinue","fragments":[{"kind":"keyword","text":"let"},{"kind":"text","text":" "},{"kind":"identifier","text":"sourceContinue"},{"kind":"text","text":": "},{"kind":"typeIdentifier","text":"gboolean","preciseIdentifier":"c:@T@gboolean"}],"abstract":[{"type":"text","text":"Use this macro as the return value of a [callback"},{"type":"reference","isActive":true,"identifier":"doc:\/\/GLib\/documentation\/GLib\/SourceFunc"},{"type":"text","text":"] to leave"},{"type":"text","text":" "},{"type":"text","text":"the [struct"},{"type":"reference","isActive":true,"identifier":"doc:\/\/GLib\/documentation\/GLib\/Source"},{"type":"text","text":"] in the main loop."}],"identifier":"doc:\/\/GLib\/documentation\/GLib\/sourceContinue","kind":"symbol","type":"topic","url":"\/documentation\/glib\/sourcecontinue"},"doc://GLib/documentation/GLib/SourceFuncs/prepare":{"role":"symbol","title":"prepare","fragments":[{"kind":"keyword","text":"var"},{"kind":"text","text":" "},{"kind":"identifier","text":"prepare"},{"kind":"text","text":": "},{"kind":"typeIdentifier","text":"GSourceFuncsPrepareFunc","preciseIdentifier":"c:@T@GSourceFuncsPrepareFunc"},{"kind":"text","text":"!"}],"abstract":[{"type":"text","text":"Called before all the file descriptors are polled. If the"},{"type":"text","text":" "},{"type":"text","text":"source can determine that it is ready here (without waiting for the"},{"type":"text","text":" "},{"type":"text","text":"results of the "},{"type":"codeVoice","code":"poll()"},{"type":"text","text":" call) it should return "},{"type":"codeVoice","code":"true"},{"type":"text","text":". It can also return"},{"type":"text","text":" "},{"type":"text","text":"a "},{"type":"codeVoice","code":"timeout_"},{"type":"text","text":" value which should be the maximum timeout (in milliseconds)"},{"type":"text","text":" "},{"type":"text","text":"which should be passed to the "},{"type":"codeVoice","code":"poll()"},{"type":"text","text":" call. The actual timeout used will"},{"type":"text","text":" "},{"type":"text","text":"be -1 if all sources returned -1, or it will be the minimum of all"},{"type":"text","text":" "},{"type":"text","text":"the "},{"type":"codeVoice","code":"timeout_"},{"type":"text","text":" values returned which were >= 0.  Since 2.36 this may"},{"type":"text","text":" "},{"type":"text","text":"be "},{"type":"codeVoice","code":"nil"},{"type":"text","text":", in which case the effect is as if the function always returns"},{"type":"text","text":" "},{"type":"codeVoice","code":"false"},{"type":"text","text":" with a timeout of -1.  If "},{"type":"codeVoice","code":"prepare"},{"type":"text","text":" returns a"},{"type":"text","text":" "},{"type":"text","text":"timeout and the source also has a ready time set, then the"},{"type":"text","text":" "},{"type":"text","text":"lower of the two will be used."}],"identifier":"doc:\/\/GLib\/documentation\/GLib\/SourceFuncs\/prepare","kind":"symbol","type":"topic","url":"\/documentation\/glib\/sourcefuncs\/prepare"},"doc://GLib/documentation/GLib/SourceFuncs/dispatch":{"role":"symbol","title":"dispatch","fragments":[{"kind":"keyword","text":"var"},{"kind":"text","text":" "},{"kind":"identifier","text":"dispatch"},{"kind":"text","text":": "},{"kind":"typeIdentifier","text":"GSourceFuncsDispatchFunc","preciseIdentifier":"c:@T@GSourceFuncsDispatchFunc"},{"kind":"text","text":"!"}],"abstract":[{"type":"text","text":"Called to dispatch the event source, after it has returned"},{"type":"text","text":" "},{"type":"codeVoice","code":"true"},{"type":"text","text":" in either its "},{"type":"codeVoice","code":"prepare"},{"type":"text","text":" or its "},{"type":"codeVoice","code":"check"},{"type":"text","text":" function, or if a ready time"},{"type":"text","text":" "},{"type":"text","text":"has been reached. The "},{"type":"codeVoice","code":"dispatch"},{"type":"text","text":" function receives a callback function and"},{"type":"text","text":" "},{"type":"text","text":"user data. The callback function may be "},{"type":"codeVoice","code":"nil"},{"type":"text","text":" if the source was never"},{"type":"text","text":" "},{"type":"text","text":"connected to a callback using [method"},{"type":"codeVoice","code":"GLib.Source.set_callback"},{"type":"text","text":"]. The"},{"type":"text","text":" "},{"type":"codeVoice","code":"dispatch"},{"type":"text","text":" function should call the callback function with "},{"type":"codeVoice","code":"user_data"},{"type":"text","text":" and"},{"type":"text","text":" "},{"type":"text","text":"whatever additional parameters are needed for this type of event source."},{"type":"text","text":" "},{"type":"text","text":"The return value of the "},{"type":"codeVoice","code":"dispatch"},{"type":"text","text":" function should be"},{"type":"text","text":" "},{"type":"text","text":"[const"},{"type":"reference","isActive":true,"identifier":"doc:\/\/GLib\/documentation\/GLib\/sourceRemove"},{"type":"text","text":"] if the source should be removed or"},{"type":"text","text":" "},{"type":"text","text":"[const"},{"type":"reference","isActive":true,"identifier":"doc:\/\/GLib\/documentation\/GLib\/sourceContinue"},{"type":"text","text":"] to keep it."}],"identifier":"doc:\/\/GLib\/documentation\/GLib\/SourceFuncs\/dispatch","kind":"symbol","type":"topic","url":"\/documentation\/glib\/sourcefuncs\/dispatch"},"doc://GLib/documentation/GLib":{"role":"collection","title":"GLib","abstract":[],"identifier":"doc:\/\/GLib\/documentation\/GLib","kind":"symbol","type":"topic","url":"\/documentation\/glib"}}}