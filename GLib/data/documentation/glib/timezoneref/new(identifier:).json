"kind":"text","text":">?) -> "},{"kind":"typeIdentifier","text":"TimeZoneRef","preciseIdentifier":"s:4GLib11TimeZoneRefV"},{"kind":"text","text":"!"}],"abstract":[{"type":"text","text":"Creates a "},{"type":"reference","isActive":true,"identifier":"doc:\/\/GLib\/documentation\/GLib\/TimeZone"},{"type":"text","text":" corresponding to "},{"type":"codeVoice","code":"identifier"},{"type":"text","text":". If "},{"type":"codeVoice","code":"identifier"},{"type":"text","text":" cannot be"},{"type":"text","text":" "},{"type":"text","text":"parsed or loaded, "},{"type":"codeVoice","code":"nil"},{"type":"text","text":" is returned."}],"identifier":"doc:\/\/GLib\/documentation\/GLib\/TimeZoneRef\/new(identifier:)","kind":"symbol","type":"topic","url":"\/documentation\/glib\/timezoneref\/new(identifier:)"},"http://www.gnu.org/s/libc/manual/html_node/TZ-Variable.html":{"title":"The GNU C Library manual","titleInlineContent":[{"type":"text","text":"The GNU C Library manual"}],"type":"link","identifier":"http:\/\/www.gnu.org\/s\/libc\/manual\/html_node\/TZ-Variable.html","url":"http:\/\/www.gnu.org\/s\/libc\/manual\/html_node\/TZ-Variable.html"},"#dzn":{"title":":mm[:ss]","titleInlineContent":[{"type":"text","text":":mm[:ss]"}],"type":"link","identifier":"#dzn","url":"#dzn"},"doc://GLib/documentation/GLib":{"role":"collection","title":"GLib","abstract":[],"identifier":"doc:\/\/GLib\/documentation\/GLib","kind":"symbol","type":"topic","url":"\/documentation\/glib"}}}